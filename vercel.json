{
  "bunVersion": "1.1.20",
  "functions": {
    "api/**/*": {
      "runtime": "bun@1.1.20",
      "includeFiles": "loan.db"
    }
  },
  "installCommand": "bun install",
  "buildCommand": "bun run build || true",
  "devCommand": "bun run dev",
  "framework": "other",
  "routes": [
    { "src": "/(.*)", "dest": "/api/index" }
  ]
}
