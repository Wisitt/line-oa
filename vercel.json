{
  "version": 2,
  "builds": [
    {
      "src": "api/index.ts",
      "use": "@vercel/node@3.1.4",
      "config": {
        "includeFiles": [
          "server.ts",
          "src/**/*",
          "loan.db"
        ]
      }
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/api/index.ts" }
  ]
}
